{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "space-before-function-paren": "off",
    "object-curly-newline": "off",
    "import/no-named-as-default": "off"
  }
}
